{
  "id": "CI-CD",
  "execute-command": "/home/ubuntu/deploy-docker.sh",
  "pass-arguments-to-command": [
    {
      "source": "payload",
      "name": "push_data.tag"
    }
  ],
  "trigger-rule": {
    "match": {
      "type": "value",
      "value": "latest",
      "parameter": {
        "source": "payload",
        "name": "push_data.tag"
      }
    }
  }
}

